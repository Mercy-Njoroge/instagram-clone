def featuredProduct(n): 
      result = 1
    for i in range(0, n): 
        result = result * ar[i] 
    return result 


ar = ['yellowShirt', 'redHat', 'blackShirt', 'bluePants', 'redHat',
           'pinkHat', 'blackShirt', 'yellowShirt', 'greenPants', 'greenPants']
n = len(ar) 
  
print(product(ar, n)) 



function featuredProduct(array){
    var resultArray = [], product;
    for(var i = 0; i < array.length; i++){
      product = 1;
      for(var j = 0; j < array.length; j++){
         if(i !== j) product *= array[j];
      }
      resultArray.push(product);
    }
    return resultArray;

var array = ['yellowShirt', 'redHat', 'blackShirt', 'bluePants', 'redHat',
